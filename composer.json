{
  "name": "hellopeterlee/scaffold",
  "description": "Extend Laravel 5.5's generators scaffold.",
  "keywords": [
    "laravel",
    "generators",
    "scaffold",
    "laravel5"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "peirenlei",
      "email": "peirenlei@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "prettus/l5-repository": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "laravel/laravel": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "HelloPeterlee\\Scaffold\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HelloPeterlee\\Scaffold\\GeneratorsServiceProvider"
      ]
    }
  }
}
